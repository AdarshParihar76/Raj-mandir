<%- include ("../partials/header") %>

	<div class="mid mt-4">
		<h1 id="head">
			Select Class
		</h1>
		<form class="form-group" action="postClass", method="post">
			<input class="form-control mt-3" type="number" placeholder="Select Class" name="class">
			<button class='btn btn-primary mt-3'> Next </button>
		</form>
	</div>
<% if(answers!=="null")	{ %>
	<div class="mid">
		<h1>
			Submitted tests
		</h1>
	</div>

	<div class="mid mt-2">
		<% answers.forEach(function(a){ %>
			<div class="mid mt-2">
				<strong><%= a.stdUsername %></strong> &nbsp;-
				
					<% for(var i = 0; i<boxes.length; i++){ %>
						<div class="testq">
							Q.&nbsp;
							<%= boxes[i].q %> ? </br>
							Ans:&nbsp;
							<%= a.ans[i] %>.
						</div>
					<% } %>					
					
			</div>
		<% }); %>
	</div>
<% } %>
	
<%- include("../partials/footer") %>