<%- include("partials/header") %>

<div class="mid mt-4" >
	<h1 id="head"> Create Your acount </h1>

	<form class="form-group" action="/register" method="post">
		<input class="form-control mt-3" type="number" placeholder="Class" name="class" required="">
		<input class="form-control mt-3" type="text" placeholder="Mobile number" name="Mobno">
		<input class="form-control mt-3" type="text" placeholder="Username" name="username" required="">
		<input class="form-control mt-3" type="password" placeholder="Create Password" name="cp" required="">
		<input class="form-control mt-3" type="password" placeholder="Confirm Password" name="password" required="">
		<button class="btn btn-primary mt-3"> Create </button>
	</form>
	<p>
		Already have an Acount ? <a href="/login" class="btn btn-outline-info"> Login </a>
	</p>
</div>

<%- include("partials/footer") %>